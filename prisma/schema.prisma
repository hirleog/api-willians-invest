generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Formulario {
  id           Int      @id @default(autoincrement())
  contatado    Boolean
  nome         String
  email        String
  cpf          String   @unique
  telefone     String
  observacao   String
  createdAt    DateTime @default(now())
}
